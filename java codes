import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int totalmarks=0;
        if(n<=0){
            System.out.println("Invalid");
        }
        else{
            for(int i=0;i<n;i++){
                int marks=sc.nextInt();
                totalmarks=totalmarks+marks;
            }
            System.out.println(totalmarks);
        }
    }
}
